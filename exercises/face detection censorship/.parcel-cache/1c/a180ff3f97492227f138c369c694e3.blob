    <title>Face Censorship</title>
    <link rel="stylesheet" href="/base.b9c847f4.css">
     <style data-parcel-key="f7d42eb1886f24d57e2b75d4e89630ba">
        * {
            box-sizing: border-box;
        }
    
        body {
            margin: 0;
        }
    
        .wrap {
            position: relative;
            min-height: 100vh;
            display: grid;
            justify-content: center;
            align-items: center;
        }
    
        .wrap>* {
            grid-column: 1;
            grid-row: 1;
        }
    
        .face {
            position: absolute;
        }
        </style>
</head>
<body>
   
    <div class="controls">
        <label for="SCALE">
            <input value="1.35" type="range" name"scale"="" min="0.3" step="0.1">
        </label>
        <label for="SIZE">
            <input value="10" type="range" name="SIZE" min="1" max="100" step="1">
        </label>
    </div>
    <div class="wrap">
        <video class="webcam"></video>
        <canvas class="video"></canvas>
        <canvas class="face"></canvas>
    </div>
    <script src="/pixelated-face.81dd1b84.js"></script>
</body>
</html>